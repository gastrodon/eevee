{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in library 'eevee'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--lib",
          "--package=eevee"
        ],
        "filter": {
          "name": "eevee",
          "kind": "lib"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug example 'xor'",
      "cargo": {
        "args": [
          "build",
          "--example=xor",
          "--package=eevee"
        ],
        "filter": {
          "name": "xor",
          "kind": "example"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
